				  CAST(p.Codprov AS INT) AS  Prov_Id,

				 CAST(p.cosuni  AS money) AS  Prod_PrecioUnitario,
				 CAST(p.costotal AS money) AS Prod_CostoTotal ,
				 p.PagImp AS  Prod_PagaImpuesto,
				 CAST(p.PACombo AS varchar(1)) AS  Prod_EsPromo,
                 ISNULL(p.ProFUpdate, GETDATE()) AS  Prod_FechaModificacion,
				 0 AS Prod_Impulsado,
				 4 AS Impu_Id,
				 1 AS Usua_Modificacion,
				 12 AS Prod_Peso,
				 4 AS UnPe_Id

				--,  u.Usua_Usuario AS ProUUpdate
		 FROM [BDD_Extena].[dbo].[PRODUCTOS] p
