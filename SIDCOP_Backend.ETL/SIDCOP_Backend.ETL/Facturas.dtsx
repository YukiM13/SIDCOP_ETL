                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT CAST(Cia AS INT) AS Fact_Id,
		CAST(DatCodigo AS INT) AS DiCl_Id,
		CAST(
			'111-' 
			+ '004-' 
			+ CASE WHEN Tipfac = 1 THEN '01' ELSE '02' END
			+ '-0000' 
			+ CAST(numfac AS varchar(10))   
		AS varchar(20)) AS Fact_Numero,
		CASE WHEN Tipfac = 1 then '01' ELSE '02' END AS Fact_TipoDeDocumento,
		21 AS RegC_Id,
		CAST(VenCodigo AS INT) AS Vend_Id,
		CAST(Tpfact AS char(2))  AS Fact_TipoVenta,
		Fechafac AS Fact_FechaEmision,
		Fechven AS Fact_FechaLimiteEmision,
	    '1001' AS Fact_RangoInicialAutorizado,
	    '1999' AS Fact_RangoFinalAutorizado,
		ImpFac AS Fact_TotalImpuesto15,
		0 AS Fact_TotalImpuesto18,
		Ftonoimp AS Fact_ImporteExento,
		FTogrimp AS Fact_ImporteGravado15,
		0 AS Fact_ImporteGravado18,
		0 AS Fact_ImporteExonerado,
		DesFact AS Fact_TotalDescuento,
		SubFac AS Fact_Subtotal,
		Totfact AS Fact_Total,
		0 AS Fact_Latitud,
		0 AS Fact_Longitud,
		Reffac AS Fact_Referencia,
		Autfac AS Fact_AutorizadoPor,
		CAST(CASE 
		WHEN Anufac = 'F' OR Anufac = '0' THEN 0
		WHEN Anufac = 'T' OR Anufac = '1' THEN 1
		ELSE NULL -- opcional, depende si quieres cubrir otros valores
		END AS INT) AS Fact_Anulado,
		1 AS Usua_Modificacion,
		GETDATE() AS Fact_FechaModificacion

	
FROM BDD_Extena.dbo.FACTURA</property>
